                                        </StackPanel>
                                        <StackPanel Margin="20,55,5,0" HorizontalAlignment="Left" >
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
                                                <!--<TextBlock Text="Total " FontFamily="Champagne &amp; Limousines" Margin="0 2" Foreground="Gray" FontSize="18" VerticalAlignment="Bottom"/>-->
                                                <TextBlock Text="{Binding TP_Nro_personas}" FontFamily="Champagne &amp; Limousines" Margin="0 2" Foreground="Gray" FontSize="22" VerticalAlignment="Bottom"/>

