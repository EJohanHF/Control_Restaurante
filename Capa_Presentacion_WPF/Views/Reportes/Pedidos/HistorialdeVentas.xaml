                                                <TextBlock Text="{Binding TP_totalpromedio}" FontFamily="Champagne &amp; Limousines" Margin="0 2" Foreground="#008297" FontSize="20" VerticalAlignment="Bottom"/>
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top">
                                                <TextBlock Text="Promedio" FontFamily="Champagne &amp; Limousines" VerticalAlignment="Center" Foreground="#008297" FontSize="13"/>
