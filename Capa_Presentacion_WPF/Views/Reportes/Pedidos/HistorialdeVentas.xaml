                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" Margin="5">
                                            <materialDesign:PackIcon Kind="Calendar" Foreground="Gray" Width="10" Height="10" VerticalAlignment="Center" Margin="5 0"/>
                                            <TextBlock Text="Ultimas " FontSize="9" Foreground="Gray"/>
                                            <TextBlock Text="{Binding UH_hora}" FontSize="9" Foreground="Gray"/>
                                            <TextBlock Text=" h" FontSize="9" Foreground="Gray"/>
                                        </StackPanel>
                                    </Grid>
                                </materialDesign:Card>
                                <!--CARD 11 ANULACION-->
                                <materialDesign:Card Width="auto" UniformCornerRadius="15" Grid.Column="2" Grid.Row="2" Height="121" Margin="0,0,10,0"  HorizontalAlignment="Stretch">
                                    <Grid>
                                        <Grid Width="35" Height="45" Background="#D03624" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20 0">
                                            <Grid.Effect>
                                                <DropShadowEffect BlurRadius="15" Color="#D03624" RenderingBias="Quality" ShadowDepth="1"/>
                                            </Grid.Effect>
                                            <materialDesign:PackIcon Kind="Store" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Foreground="White" Width="20" Height="20"/>
                                        </Grid>
                                        <Label Content=" VENTAS  " HorizontalContentAlignment="Center" HorizontalAlignment="Left" FontFamily="Champagne &amp; Limousines" Margin="50,0,0,0" 
                                    VerticalAlignment="Top"  FontSize="20"  Foreground="Gray" Height="auto"/>
                                        <Label Content=" ANULACIÓN  " HorizontalContentAlignment="Center" HorizontalAlignment="Right" FontFamily="Champagne &amp; Limousines" Margin="0" VerticalAlignment="Top"  FontSize="15" Background="#D03624"  Foreground="White" Height="25"/>
                                        <StackPanel Margin="0,70,8,0" HorizontalAlignment="Right" >
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top">
                                                <TextBlock Text="S/ " FontFamily="Champagne &amp; Limousines" Margin="0 2" Foreground="Gray" FontSize="20" VerticalAlignment="Bottom"/>
                                                <TextBlock Text="{Binding VAMonto}" FontFamily="Champagne &amp; Limousines" Margin="0 2" Foreground="Gray" FontSize="20" VerticalAlignment="Bottom"/>
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top">
                                                <TextBlock Text="Total" FontFamily="Champagne &amp; Limousines" VerticalAlignment="Center" Foreground="Gray" FontSize="13"/>
                                            </StackPanel>
                                        </StackPanel>
                                        <StackPanel Margin="10,50,5,0" HorizontalAlignment="Left" >
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
                                                <TextBlock Text="{Binding Nanulados}" FontFamily="Champagne &amp; Limousines" Margin="0 2" Foreground="#D03624" FontSize="22" VerticalAlignment="Bottom"/>
                                                <TextBlock Text="Mesas " FontFamily="Champagne &amp; Limousines" Margin="10,0,0,0" Foreground="#D03624" FontSize="13" VerticalAlignment="Center"/>
                                                <Button Style="{StaticResource MaterialDesignOutlinedButton}" Command="{Binding AbrirDialogDetVentasAnualdos}" Foreground="#D03624" Background="Transparent"
                                                Content="{materialDesign:PackIcon Kind=Eye}" ToolTip="Detalle" Padding="0" Height="25" Width="30" Margin="0,0,5,0" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
                                                <TextBlock Text="{Binding Panulados}" FontFamily="Champagne &amp; Limousines" Margin="0 2" Foreground="#D03624" FontSize="22" VerticalAlignment="Bottom"/>
                                                <TextBlock Text="Productos " FontFamily="Champagne &amp; Limousines" Margin="10,0,0,0" Foreground="#D03624" FontSize="13" VerticalAlignment="Center"/>
                                                <Button Style="{StaticResource MaterialDesignOutlinedButton}" Command="{Binding AbrirDialogDetProdAnualdos}" Foreground="#D03624" Background="Transparent"
                                                Content="{materialDesign:PackIcon Kind=Eye}" ToolTip="Detalle" Padding="0" Height="25" Width="30" Margin="0,0,5,0" />
                                            </StackPanel>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" Margin="5">
                                            <materialDesign:PackIcon Kind="Calendar" Foreground="Gray" Width="10" Height="10" VerticalAlignment="Center" Margin="5 0"/>
                                            <TextBlock Text="Ultimas " FontSize="9" Foreground="Gray"/>
                                            <TextBlock Text="{Binding UH_hora}" FontSize="9" Foreground="Gray"/>
                                            <TextBlock Text=" h" FontSize="9" Foreground="Gray"/>
                                        </StackPanel>
                                    </Grid>
                                </materialDesign:Card>
                                <!--CARD 12 TIEMPO DE DESPACHO-->
                                <materialDesign:Card Width="auto" UniformCornerRadius="15" Grid.Column="4" Grid.Row="2" Height="121" Margin="0,0,10,0"  HorizontalAlignment="Stretch">
                                    <Grid>
                                        <Grid Width="35" Height="25" Background="#66CD04" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20 0">
                                            <Grid.Effect>
                                                <DropShadowEffect BlurRadius="15" Color="#66CD04" RenderingBias="Quality" ShadowDepth="1"/>
                                            </Grid.Effect>
                                            <materialDesign:PackIcon Kind="Store" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Foreground="White" Width="20" Height="20"/>
                                        </Grid>
                                        <TextBlock TextWrapping="Wrap" Text="VENTAS X CANAL  " MaxWidth="200" HorizontalAlignment="Left" FontFamily="Champagne &amp; Limousines" 
                                    Margin="60,0,0,0" VerticalAlignment="Top" TextAlignment="Left"  FontSize="20"  Foreground="Gray" Height="auto"/>
                                        <Label Content=" S/ " HorizontalContentAlignment="Center" HorizontalAlignment="Right" FontFamily="Champagne &amp; Limousines" Margin="0" 
                                    VerticalAlignment="Top"  FontSize="15" Background="#66CD04"  Foreground="White" Height="25"/>
                                        <Grid Margin="5,25,0,0" HorizontalAlignment="Center">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="90"/>
                                                <ColumnDefinition Width="70"/>
                                                <ColumnDefinition Width="70"/>
                                            </Grid.ColumnDefinitions>
                                            <Label Grid.Column="0" Content="Canal"/>
                                            <Label Grid.Column="1" Content="Cantidad"/>
                                            <Label Grid.Column="2" Content="Importe"/>
                                        </Grid>
                                        <Grid Margin="10,45,0,0" HorizontalAlignment="Center" >
                                            <Grid HorizontalAlignment="Center" VerticalAlignment="Top" MaxHeight="50">
                                                <ScrollViewer Grid.Row="2" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" 
                                                    Grid.Column="2" VerticalAlignment="Stretch">
                                                    <ItemsControl ItemsSource="{Binding DataVentasxCanal}">
                                                        <ItemsControl.ItemTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="90"/>
                                                                        <ColumnDefinition Width="50"/>
                                                                        <ColumnDefinition Width="70"/>
                                                                    </Grid.ColumnDefinitions>
                                                                    <TextBlock Grid.Column="0" Text="{Binding canal_venta}" FontFamily="Champagne &amp; Limousines" 
                                                                        Foreground="Black" FontSize="13" HorizontalAlignment="Left"/>
                                                                    <TextBlock Grid.Column="1" Text="{Binding cantidad_venta}" FontFamily="Champagne &amp; Limousines" 
                                                                        Foreground="#66CD04" FontSize="13" HorizontalAlignment="Center"/>
                                                                    <TextBlock Grid.Column="2" Text="{Binding importe_venta}" FontFamily="Champagne &amp; Limousines" 
                                                                        Foreground="Red" FontSize="13" HorizontalAlignment="Right"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </ItemsControl.ItemTemplate>
                                                    </ItemsControl>
                                                </ScrollViewer>
                                            </Grid>
                                        </Grid>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" Margin="5">
                                            <materialDesign:PackIcon Kind="Calendar" Foreground="Gray" Width="10" Height="10" VerticalAlignment="Center" Margin="5 0"/>
                                            <TextBlock Text="Ultimas " FontSize="9" Foreground="Gray"/>
                                            <TextBlock Text="{Binding UH_hora}" FontSize="9" Foreground="Gray"/>
                                            <TextBlock Text=" h" FontSize="9" Foreground="Gray"/>
                                        </StackPanel>
                                    </Grid>
                                </materialDesign:Card>

                                <!--GRAFICOS-->
                                <Grid Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="6" HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" MinWidth="280"/>
                                        <ColumnDefinition Width="*" MinWidth="280"/>
                                        <ColumnDefinition Width="*" MinWidth="280"/>
                                    </Grid.ColumnDefinitions>
                                    <!--PLATOS POR AREA-->
                                    <materialDesign:Card Width="auto" UniformCornerRadius="15" Grid.Column="0" Grid.Row="3"  Margin="0,0,10,0"  
                                            HorizontalAlignment="Stretch" Height="280" >
                                        <Grid>
                                            <Label Content=" PLATOS POR ÁREA  " HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Champagne &amp; Limousines" 
                                    Margin="0" VerticalAlignment="Top"  FontSize="15" Background="#09B299"  Foreground="White" Height="25"/>
                                            <Wpf:PieChart  LegendLocation="Bottom" FontSize="11" Foreground="Gray" Series="{Binding dataPlatosxArea}"  
                                            Hoverable="False"  Margin="0,35,0,0" >
                                                <Wpf:PieChart.Resources>
                                                    <Style TargetType="Wpf:PieSeries">
                                                        <Setter Property="DataLabels" Value="True"></Setter>
                                                        <Setter Property="FontSize" Value="16"></Setter>
                                                        <Setter Property="Foreground" Value="#303030"></Setter>
                                                        <Setter Property="LabelPosition" Value="InsideSlice"></Setter>
                                                        <Setter Property="StrokeThickness" Value="1"></Setter>
                                                    </Style>
                                                </Wpf:PieChart.Resources>
                                            </Wpf:PieChart>
                                        </Grid>
                                    </materialDesign:Card>

                                    <!--INGRESO POR CANALES DE VENTA-->
                                    <materialDesign:Card Width="auto" UniformCornerRadius="15" Grid.Column="1" Grid.Row="3"  Margin="0,0,10,0"  HorizontalAlignment="Stretch" Height="280" >
                                        <Grid>
                                            <Label Content=" INGRESO POR CANALES DE VENTA  " HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Champagne &amp; Limousines" 
                                    Margin="0" VerticalAlignment="Top"  FontSize="15" Background="#0987B2"  Foreground="White" Height="25"/>
                                            <Wpf:PieChart LegendLocation="Bottom" Foreground="Gray" Series="{Binding dataCanaldeVenta}" Hoverable="False" 
                                            DataTooltip="{x:Null}" Margin="0,35,0,30">
                                                <Wpf:PieChart.Resources>
                                                    <Style TargetType="Wpf:PieSeries">
                                                        <Setter Property="DataLabels" Value="True"></Setter>
                                                        <Setter Property="FontSize" Value="15"></Setter>
                                                        <Setter Property="Foreground" Value="#303030"></Setter>
                                                        <Setter Property="LabelPosition" Value="InsideSlice"></Setter>
                                                        <Setter Property="StrokeThickness" Value="1"></Setter>
                                                    </Style>
                                                </Wpf:PieChart.Resources>
                                            </Wpf:PieChart>
                                        </Grid>
                                    </materialDesign:Card>

                                    <!--VENTAS POR HORA-->
                                    <materialDesign:Card Width="auto" UniformCornerRadius="15" Grid.Column="2" Grid.Row="3"  Margin="0,0,10,0"  HorizontalAlignment="Stretch" Height="280" >
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="30"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <Label Content=" VENTAS POR HORA  " Grid.Row="0" HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Champagne &amp; Limousines" Margin="0" VerticalAlignment="Top"  FontSize="15" Background="#145776"  Foreground="White" Height="25"/>
                                            <Wpf:CartesianChart Series="{Binding dataVentasxHora}"  LegendLocation="None" Margin="0,0,0,10" Grid.Row="1">
                                                <Wpf:CartesianChart.SeriesColors>
                                                    <Wpf:ColorsCollection>
                                                        <Color>#145776</Color>
                                                    </Wpf:ColorsCollection>
                                                </Wpf:CartesianChart.SeriesColors>
                                                <Wpf:CartesianChart.AxisX>
                                                    <Wpf:AxesCollection>
                                                        <Wpf:Axis Labels="{Binding Labels1}" >
                                                            <Wpf:Axis.Separator>
                                                                <Wpf:Separator Step="1" />
                                                            </Wpf:Axis.Separator>
                                                        </Wpf:Axis>
                                                    </Wpf:AxesCollection>
                                                </Wpf:CartesianChart.AxisX>
                                                <Wpf:CartesianChart.AxisY>
                                                    <Wpf:Axis Title="Monto" LabelFormatter="{Binding Formatter}"/>
                                                </Wpf:CartesianChart.AxisY>
                                            </Wpf:CartesianChart>
                                        </Grid>
                                    </materialDesign:Card>
                                </Grid>

                            </Grid>
                        </Grid>
                    </Grid>
                </ScrollViewer>

                <!--DETALLADO-->
                <Grid>
                    <Grid.Style>
                        <Style TargetType="Grid">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding Operacion}" Value="Detallado">
                                    <Setter Property="FrameworkElement.Visibility" Value="Visible"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Operacion}" Value="Consolidado">
                                    <Setter Property="FrameworkElement.Visibility" Value="Collapsed"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Grid.Style>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="70"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Column="0" Margin="0,0,18,0">

                    </Grid>
                    <Grid Grid.Row="1" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="100"/>
                        </Grid.RowDefinitions>
                        <DataGrid x:Name="DatagridPed" SelectedItem="{Binding SelectedDetalle}" FontSize="15" Grid.Row="0" Grid.Column="1"
                                Margin="10" Width="auto" AutoGenerateColumns="False" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto"
                                IsReadOnly="True" CanUserAddRows="False" CanUserDeleteRows="False"
                                ItemsSource="{Binding DataDetallePedido}" HorizontalAlignment="Stretch">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseUp">
                                    <i:InvokeCommandAction  
                        Command="{Binding RelativeSource={RelativeSource AncestorType=UserControl}, 
                                        Path=DataContext.ClickDetalleCommand}">
                                    </i:InvokeCommandAction>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <DataGrid.Columns>
                                <DataGridTemplateColumn Header="" >
                                    <DataGridTemplateColumn.CellStyle>
                                        <Style TargetType="{x:Type DataGridCell}" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                            <Setter Property="Margin" Value="0"/>
                                            <Setter Property="Padding" Value="0"/>
                                            <Setter Property="HorizontalAlignment" Value="Center"/>
                                            <Setter Property="BorderThickness" Value="0"/>
                                        </Style>
                                    </DataGridTemplateColumn.CellStyle>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <Button  Command="{Binding Path=DataContext.AbrirDialogDetPedido,
                                                    RelativeSource= {RelativeSource FindAncestor,AncestorType={x:Type DataGrid}} }" Foreground="#9A03FC" Background="Transparent"
                                                    CommandParameter="{Binding id_ped}"
                                                    Content="{materialDesign:PackIcon Kind=Eye}" ToolTip="Detalle pedido" Padding="0" Height="25" Width="30" Margin="0,0,5,0" >
                                                    <Button.Style>
                                                        <Style TargetType="{x:Type Button}">
                                                            <Setter Property="Visibility" Value="Visible"/>
                                                            <Setter Property="Background" Value="White"/>
                                                            <Setter Property="BorderBrush" Value="White"/>
                                                            <Setter Property="HorizontalContentAlignment" Value="Center"/>
                                                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Binding nom_estado_f}" Value="ANULADO">
                                                                    <Setter Property="Visibility" Value="Hidden"/>
                                                                </DataTrigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </Button.Style>
                                                </Button>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTextColumn Header="ID" Binding="{Binding id_ped}" FontSize="13"/>
                                <DataGridTextColumn Header="N° Diario" Binding="{Binding num_dia_ped}" FontSize="13"/>
                                <DataGridTextColumn Header="Mesa" Binding="{Binding nom_mesa}" FontSize="13"/>
                                <DataGridTextColumn Header="Descuento" Binding="{Binding desc_ped}" FontSize="13"/>
                                <DataGridTextColumn Header="Subtotal" Binding="{Binding subtotal_ped}" FontSize="13"/>
                                <DataGridTextColumn Header="Precio total" Binding="{Binding total_ped}" FontSize="13"/>
                                <DataGridTextColumn Header="F. Pedido" Binding="{Binding f_ped}" FontSize="13"/>
                                <DataGridTemplateColumn IsReadOnly="True" Header="Estado Financiero">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <materialDesign:Chip Width="110" HorizontalAlignment="Center" Cursor="Arrow" FontSize="13" Foreground="#EFEFEF" 
                                                                     Background="{Binding color_chips}" Content="{Binding nom_estado_f}"/>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <!--<DataGridTextColumn Header="Importe" Binding="{Binding impor_ped}" FontSize="13"/>-->
                                <DataGridTextColumn Header="Nro Personas" Binding="{Binding nro_personas}" FontSize="13"/>
                                <DataGridTemplateColumn Header="Cliente" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding nomllevar}" FontSize="13" TextWrapping="Wrap" MaxWidth="150"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="Telefono" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding telefcli}" FontSize="13" TextWrapping="Wrap" MaxWidth="150"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="Tipo Descuento" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding nom_tip_desc}" FontSize="13" TextWrapping="Wrap" MaxWidth="120"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTextColumn Header="Forma de pago" Binding="{Binding nom_fpago}" FontSize="13"/>
                                <DataGridTextColumn Header="Datos de Tarjeta" Binding="{Binding nro_tarjeta}" FontSize="13"/>
                                <DataGridTextColumn Header="Mozo" Binding="{Binding nom_emple}" FontSize="13"/>
                                <DataGridTextColumn Header="Propina" Binding="{Binding propina}" FontSize="13"/>
                                <DataGridTextColumn Header="Forma Pago Propina" Binding="{Binding TPpropina}" FontSize="13"/>
                                <DataGridTextColumn Header="Usuario" Binding="{Binding nom_usu}" FontSize="13"/>
                                <DataGridTextColumn Header="Serie de Documento" Binding="{Binding nom_tipo_Doc}" FontSize="13"/>
                                <DataGridTextColumn Header="Importe" Binding="{Binding importe_Total_Doc_Elec}" FontSize="13"/>
                            </DataGrid.Columns>
                        </DataGrid>
                        <Grid Grid.Column="0" Grid.Row="1">

                            <Grid HorizontalAlignment="Center">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="90"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="238"/>
                                    <ColumnDefinition Width="238"/>
                                    <ColumnDefinition Width="238"/>
                                </Grid.ColumnDefinitions>
                                <Button x:Name="btnBolac" Width="200" Height="50" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" 
                                Command="{Binding OpenDialogBolFactCommand}" IsEnabled="{Binding FacturacionEnable }">
                                    <StackPanel Orientation="Horizontal" Width="170">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="18"/>
                                                <RowDefinition Height="18"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="40"/>
                                                <ColumnDefinition Width="130"/>
                                            </Grid.ColumnDefinitions>

                                            <Image Grid.Column="0" Grid.Row="0" Grid.RowSpan="2" Source="/Resources/Images/botones/imprimir cuenta.png" Stretch="UniformToFill" Height="40"/>
                                            <TextBlock Text="FACTURA" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1"   FontSize="15" Height="auto" />
                                            <TextBlock Text="BOLETA" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="0" Margin="0,0,0,0" FontSize="15"  Height="auto"/>
                                        </Grid>

                                    </StackPanel>
                                </Button>
                                <Button Width="200" Height="50" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" 
                            Command="{Binding ImprimirCuentaCommand}"  >
                                    <StackPanel Orientation="Horizontal" Width="170" >
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="18"/>
                                                <RowDefinition Height="18"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="40"/>
                                                <ColumnDefinition Width="130"/>
                                            </Grid.ColumnDefinitions>

                                            <Image Grid.Column="0" Grid.Row="0" Grid.RowSpan="2" Source="/Resources/Images/botones/imprimir cuenta.png" Stretch="UniformToFill" Height="40"/>
                                            <TextBlock Text="IMPRIMIR" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="0"   FontSize="16" Height="auto" />
                                            <TextBlock Text="CUENTA" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1" Margin="0,0,0,0" FontSize="16"  Height="auto"/>
                                        </Grid>

                                    </StackPanel>
                                </Button>


                                <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="2" >
                                    <Button Width="55" Height="50" Padding="0" Grid.Row="1" Grid.Column="1"  HorizontalAlignment="Left" 
                                        Command="{Binding ExportaExcelCommand}" ToolTip="Excel">
                                        <materialDesign:PackIcon Kind="FileExcel" Foreground="#ff8e1e"  Height="35" Width="40" />
                                    </Button>
                                    <TextBlock Text=" EXPORTAR "  HorizontalAlignment="Center" VerticalAlignment="Center"   FontSize="17" Height="auto" Foreground="Black" />

                                    <Button Width="55" Height="50" HorizontalAlignment="Right" Padding="0"
                                        Command="{Binding ExportaPDFCommand}" ToolTip="PDF" Foreground="#ff8e1e">
                                        <materialDesign:PackIcon Kind="PdfBox"  Height="40" Width="40" />
                                    </Button>
                                </StackPanel>
                            </Grid>
                        </Grid>
                    </Grid >
                </Grid>
            </Grid>
        </ContentControl>
    </Grid>
</UserControl>
